<h1>Relatório de Vendas</h1>

<%= link_to "Relatório Geral de Vendas", vendas_geral_path, class: "btn btn-primary" %>

<hr>

<%= form_with(url: cartlist_data_path, method: "get") do |f| %>
  <div class="form-group">
    <%= f.label :data %>
    <%= f.date_field :data, placeholder: "Data" %>
  </div>

  <div class="form-group">
    <%= f.submit "Filtrar", class: "btn btn-primary" %>
  </div>
<% end %>

